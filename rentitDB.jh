

entity Product {
	id Long,
	title String,
    image String
}

entity Booking {
	id Long,
    dateTime String,
    paymentType String,
}

entity Rate {
	id Long,
    price BigDecimal,
    name String
}

entity Comment {
	id Long,
    text String
}


entity TimeSlot {
	id Long,
    dateTime String,
    duration Long,
}

entity UserAddress {
	id Long,
	streetAddress String,
	postalCode String,
	city String,
	stateProvince String
}

